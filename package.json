{
  "name": "@cutting-mat/vue-store",
  "version": "2.0.2",
  "description": "Simpler Vue state management plug-in",
  "private": false,
  "files": ["dist"],
  "main": "./dist/store.umd.js",
  "module": "./dist/store.es.js",
  "exports": {
    ".": {
      "import": "./dist/store.es.js",
      "require": "./dist/store.umd.js"
    }
  },
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "build-doc": "vite build --target es2016",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue-demi": "^0.12.4"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.9",
    "vue": "^2.6.14 || >=3.2.31"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^0.2.7",
    "element-plus": "^2.0.2",
    "@vitejs/plugin-vue": "^2.2.0",
    "vite": "^2.8.0",
    "vue": "^3.2.31"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cutting-mat/vue-store.git"
  },
  "keywords": [
    "store",
    "vuex",
    "vue-store"
  ],
